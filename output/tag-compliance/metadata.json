{
  "policy": {
    "name": "tag-compliance",
    "resource": "aws.ec2",
    "description": "Schedule a resource that does not meet tag compliance policies\nto be stopped in four days.\n",
    "filters": [
      {
        "State.Name": "running"
      },
      {
        "tag:Environment": "absent"
      },
      {
        "tag:AppId": "absent"
      },
      {
        "or": [
          {
            "tag:OwnerContact": "absent"
          },
          {
            "tag:DeptID": "absent"
          }
        ]
      }
    ],
    "actions": [
      {
        "type": "mark-for-op",
        "op": "stop",
        "days": 4
      }
    ]
  },
